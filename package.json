    "dotenv": "^8.2.0",
    "micro": "^9.3.4",
    "microrouter": "^3.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve-handler": "^6.1.3",
    "tailwindcss": "^1.7.4",
    "ws": "^7.3.1"
  },
  "devDependencies": {
